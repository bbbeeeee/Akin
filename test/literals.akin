# Literals of the language, each are stored in a ValueObject instance.

print(true)
# => true
print(true.class.name)
# => TrueClass

print(nil)
# => null
print(nil.class.name)
# => NilClass

print(false)
# => false
print(false.class.name)
# => FalseClass

print("hi".class.name)
# => String

print(1.class.name)
# => Integer

print(1.2.class.name)
# => Float